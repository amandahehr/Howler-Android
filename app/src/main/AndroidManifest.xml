<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.amanda.howler">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TrackingTimer" />
        <activity android:name=".TrackingFinished" />
        <activity android:name=".ContactList"
            android:label="Contact List"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".AddContact"
            android:label="Add a Contact"
            android:parentActivityName=".ContactList"/>
        <activity android:name=".ViewContact"
            android:label="View Contact"
            android:parentActivityName=".ContactList">
        </activity>
    </application>

</manifest>